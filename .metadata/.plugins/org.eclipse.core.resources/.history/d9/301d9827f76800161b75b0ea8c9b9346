
public class MovieTester {
	public static void main(String[] args) {

		Movie [] someMovies = new Movie[5];
		
		someMovies[0] = new Movie("Terminator", 1982);
		someMovies[1] = new Movie("Dune", 1984);
		someMovies[2] = new Movie("Beggars Banquete");
		someMovies[3] = new Movie("Ivan Meets GI Joe");
		someMovies[4] = new Movie("Assimilate", 2016);
		
		for(int i = 0; i < someMovies.length; i++) {
			System.out.println(someMovies[i]);
		}
	}
}
