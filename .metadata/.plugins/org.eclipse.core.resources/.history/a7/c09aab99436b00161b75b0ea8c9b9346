
public class BoxR {
	private Object thing;
	private int thingCount;
	
	public BoxR(Object firstThing) {
		this.thing = firstThing;
		this.thingCount = 1;
	}

	public Object getContents() {
		return thing;
	}

	public int getCount() {
		return thingCount;
	}
	
	@Override 
	public String toString() {
		return thing.toString() + " (item " + thingCount + ")";
	}
}
