
public class VowelsLetters {
	while(!letters.isEmpty()) {
		String currentLetter = letters.remove();
		if (currentLetter == 'a' || 
				currentLetter == 'e' || 
				currentLetter == 'i' || 
				currentLetter == 'o' ||
				currentLetter == 'u')
			vowels.add(currentLetter);
	}
	
	int aCount = 0, eCount = 0, iCount = 0, oCount = 0, uCount = 0;
	
	
}
