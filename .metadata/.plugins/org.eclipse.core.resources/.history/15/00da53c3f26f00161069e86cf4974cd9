
public class OrderedPair<T> implements Pairable<T> {
	private T first, second;
	
	public OrderedPair(T firstItem, T secondItem ) {
		this.first = firstItem;
		this.second = secondItem;
	}
	
	public T getFirst() {
		return first;
	}
	
	public T getSecond() {
		return second;
	}
	
	@Override
	public String toString() {
		return "(" + first + ", " + second + " )";
	}

}
