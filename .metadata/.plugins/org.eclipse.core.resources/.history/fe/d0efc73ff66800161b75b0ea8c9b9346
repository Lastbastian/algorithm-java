
public class Documentary extends Movie {
	private String topic;
	
	public Documentary(String title, int releaseYear, String topic) {
		super(title, releaseYear);
		this.topic = topic;
	}

	public String getTopic() {
		return topic;
	}

	public void setTopic(String topic) {
		this.topic = topic;
	}
	
	
	

}
